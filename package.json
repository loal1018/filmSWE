{
  "name": "buch",
  "version": "2024.04.0",
  "description": "Beispiel",
  "author": "Juergen Zimmermann",
  "private": true,
  "license": "GPL-3.0",
  "exports": "./dist/src/main.js",
  "type": "module",
  "engines": {
    "node": ">=20.11.1",
    "npm": ">=10.3.0"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "nest build",
    "start": "node dist/src/main.js",
    "pre:dev": "npm run copy-resources",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "test:ORIG": "jest",
    "pretest": "npm run copy-resources",
    "test": "set LOG_DEFAULT=true&& jest --runInBand",
    "test:istanbul": "npm t -- --coverage",
    "test:logdebug": "jest --runInBand",
    "test:dbserver": "set LOG_DEFAULT=true&&set START_DB_SERVER=true&&jest --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "asciidoctor": "node scripts/asciidoctor",
    "clean": "rimraf dist log/server.log",
    "copy-resources": "node scripts/copy-resources",
    "depcheck": "depcheck",
    "eslint": "eslint src __tests__",
    "eslint:debug": "eslint --debug src __tests__",
    "invoke-webrequest": "pwsh -Command Invoke-WebRequest https://localhost:3000/rest/1 -SslProtocol Tls13 -SkipCertificateCheck",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "madge": "madge --circular src/main.ts",
    "madge:svg": "madge --image dependencies.svg src/main.ts",
    "prettier": "prettier --cache --write \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "prettier:check": "prettier --cache --check \"src/**/*.ts\" \"__tests__/**/*.ts\"",
    "readme2html": "node scripts/readme2html",
    "revealjs": "asciidoctor-revealjs .extras/doc/folien/folien.adoc",
    "snyk:auth": "echo https://app.snyk.io/account && snyk auth",
    "snyk": "snyk test",
    "sonar": "node scripts/sonar-scanner",
    "typedoc": "typedoc",
    "tsc": "tsc --pretty"
  },
  "keywords": [
    "Node",
    "npm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "Axios",
    "TypeScript",
    "ESLint",
    "PostgreSQL",
    "TypeORM",
    "JWT",
    "Passport",
    "argon2",
    "Ajv",
    "CSP",
    "Clickjacking",
    "CORS",
    "pino",
    "nodemailer",
    "Jest",
    "ts-jest"
  ],
